{
    "name": "qobo/cakephp-csv-migrations",
    "description": "CSV Migrations plugin for CakePHP",
    "type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Qobo Ltd",
			"email": "info@qobo.biz",
			"homepage": "https://www.qobo.biz",
			"role": "Developer"
		}
	],
    "require": {
        "eluceo/ical": "^0.11.0",
        "qobo/cakephp-translations": "^7.0",
        "qobo/cakephp-utils": "^6.0",
        "symfony/expression-language": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "replace": {
        "qobo/cakephp-csv-views": "1.0.0 - 1.0.5"
    },
    "autoload": {
        "psr-4": {
            "CsvMigrations\\": "src",
            "CsvMigrations\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CsvMigrations\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
