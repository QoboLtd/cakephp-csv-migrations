{
    "name": "qobo/cakephp-csv-migrations",
    "description": "CSV Migrations plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": ">3.0.0 <4.0",
        "cakephp/migrations": "~1.0",
        "league/csv": "^8.1",
        "piwik/ini": "^1.0",
        "burzum/cakephp-file-storage": "^1.1",
        "friendsofcake/crud": "^4.3",
        "friendsofcake/bootstrap-ui": "~0.3",
        "symfony/expression-language": "^3.1",
        "admad/cakephp-jwt-auth": "^2.0",
        "muffin/trash": "^1.1",
        "eluceo/ical": "^0.11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "replace": {
        "qobo/cakephp-csv-views": "1.0.0 - 1.0.5"
    },
    "autoload": {
        "psr-4": {
            "CsvMigrations\\": "src",
            "CsvMigrations\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CsvMigrations\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
